@model IEnumerable<SDGE.ApplicationCore.Entity.MembroEvento>

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>MembroEvento</h4>
    <hr />
    @foreach (var item in Model)
    {
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Comissao)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(modelitem => item.Comissao)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Evento)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(modelitem => item.Evento.Titulo)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Membro)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(modelitem => item.Membro.Nome)
            </dd>
        </dl>
    }
    </div>
<div>
    @foreach (var item in Model)
    {
    <a asp-action="Edit" asp-route-id="@item.MembroEventoId">Edit</a> 
    }|
    <a asp-action="Index">Back to List</a>
</div>
