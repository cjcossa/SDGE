@model IEnumerable<SDGE.ApplicationCore.Entity.EventoParticipante>

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>EventoParticipante</h4>
    <hr />
    <dl class="row">
        @foreach (var item in Model)
        {
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Comprovativo)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(modelitem => item.Comprovativo)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Evento)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(modelitem => item.Evento.Titulo)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Participante)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(modelitem => item.Participante.Nome)
            </dd>
        }
        </dl>
</div>
<div>
    @foreach (var item in Model)
    {
        <a asp-action="Edit" asp-route-id="@item.EventoParticipanteId">Edit</a>
    }|
<a asp-action="Index">Back to List</a>
</div>
