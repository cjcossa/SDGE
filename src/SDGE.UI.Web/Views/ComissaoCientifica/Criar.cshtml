@model SDGE.UI.Web.Models.ComissaoCientificaViewModel

@{
    ViewData["Title"] = "Criar";
}

<div class="row">
    <div class="col-md-2"></div>
    <div class="col-md-8">
        <div class="card">
            <div class="card-header card-header-primary">
                <h4 class="card-title">Criar a comissão científica</h4>
                <p class="card-category">Comissão responsável por avaliar as submissões.</p>
            </div>
            <div class="card-body">
                <form asp-action="Criar">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                                <label asp-for="Codigo" class="bmd-label-floating"></label>
                                <input asp-for="Codigo" class="form-control" disabled />
                                <input type="hidden" asp-for="Codigo" class="form-control" />
                                <span asp-validation-for="Codigo" class="text-danger"></span>
                            </div>
                            <div class="form-group text-right">
                                <button asp-action="Index" class="btn btn-primary btn-sm waves-effect">Voltar</button>
                                <input type="submit" id="criar" value="Criar" class="btn btn-outline-primary btn-sm" />
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>


